.pos 0
init:   irmovl $10, %ecx    # Initialize loop counter
        irmovl $0, %eax     # Initialize result register
        irmovl $1, %edx     # Initialize increment value

loop_start:
        addl %edx, %eax     # Increment result
        loop loop_start     # Decrement ecx and loop if not zero

        halt 